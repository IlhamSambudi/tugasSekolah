public class SINGLED1 {

    public static void main(String[] args) {
        int nilai[] = new int[3];
        nilai[0] = 80;
        nilai[1] = 80;
        nilai[2] = 95;
        double MEAN = 0.0;
        for (int i = 0; i < nilai.length; i++) {
            MEAN += nilai[i];
        }
        MEAN /= nilai.length;
        System.out.println("Nilai rata-rata = " + MEAN);
    }
}
